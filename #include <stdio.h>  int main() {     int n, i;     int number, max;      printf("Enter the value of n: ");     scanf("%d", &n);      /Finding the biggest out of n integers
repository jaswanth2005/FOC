#include <stdio.h>

int main() {
    int n, i;
    int number, max;

    printf("Enter the value of n: ");
    scanf("%d", &n);

    // Initialize max to the smallest possible value
    max = -2147483647;

    for (i = 1; i <= n; i++) {
        printf("Enter number %d: ", i);
        scanf("%d", &number);

        if (number > max) {
            max = number;
        }
    }

    printf("The biggest number is: %d\n", max);

    return 0;
}
